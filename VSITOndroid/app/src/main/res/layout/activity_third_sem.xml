<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.ash.vsitondroid.ThirdSem">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mathematics -III"
        android:id="@+id/button13"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="134dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Computer Architecture"
        android:id="@+id/button14"
        android:layout_alignTop="@+id/button13"
        android:layout_toEndOf="@+id/button13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Button"
        android:id="@+id/button15"
        android:layout_alignTop="@+id/button14"
        android:layout_toEndOf="@+id/button14" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Front End Design Tool VB.Net"
        android:id="@+id/button16"
        android:layout_below="@+id/button13"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Principles of Accounting"
        android:id="@+id/button19"
        android:layout_below="@+id/button16"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Object Oriented Programming using C++"
        android:id="@+id/button20"
        android:layout_below="@+id/button19"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Practical – V .NET Lab"
        android:id="@+id/button21"
        android:layout_below="@+id/button20"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Practical – VI C++ Lab"
        android:id="@+id/button22"
        android:layout_below="@+id/button21"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Software Development Skills"
        android:id="@+id/button23"
        android:layout_below="@+id/button22"
        android:layout_alignParentStart="true" />
</RelativeLayout>
